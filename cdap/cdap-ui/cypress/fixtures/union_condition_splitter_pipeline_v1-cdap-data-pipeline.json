                "inputSchema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"product\",\"type\":\"string\"},{\"name\":\"price\",\"type\":\"string\"}]}"
            },
            {
                "name": "String Prices",
                "plugin": {
                    "name": "File",
                    "type": "batchsink",
                    "label": "String Prices",
                    "artifact": {
                        "name": "core-plugins",
                        "version": "[2.3.0-SNAPSHOT, 3.0.0-SNAPSHOT)",
                        "scope": "SYSTEM"
                    },
                    "properties": {

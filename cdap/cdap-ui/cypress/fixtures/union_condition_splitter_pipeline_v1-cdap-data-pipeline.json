    },
    "description": "",
    "name": "union_condition_splitter_pipeline_v1",
    "config": {
        "resources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "driverResources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "connections": [
            {
                "from": "File",
                "to": "CSVParser"
            },
            {
                "from": "CSVParser",
                "to": "JavaScript3"
            },
            {
                "from": "JavaScript3",
                "to": "UnionSplitter"
            },
            {
                "from": "UnionSplitter",
                "to": "String JavaScript Emitter",
                "port": "string"
            },
            {
                "from": "String JavaScript Emitter",
                "to": "Conditional"
            },
            {
                "from": "Conditional",
                "to": "String Prices",
                "condition": "true"
            },
            {
